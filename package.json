{
    "name": "TPM-rewrite",
    "version": "0.0.1",
    "main": "index.js",
    "bin": "index.js",
    "scripts": {
        "start": "node index.js"
    },
    "pkg": {
        "scripts": [
            "index.js",
            "logger.js",
            "config.js",
            "TPM-bot/*",
            "webpage/script.js"
        ],
        "assets": [
            "node_modules/axios/**/*",
            "logs/**/*",
            "webpage/index.html"
        ],
        "targets": [
            "node18-win-x64",
            "node18-macos-x64",
            "node18-linux-x64"
        ]
    },
    "dependencies": {
        "axios": "^1.7.7",
        "body-parser": "^1.20.3",
        "cors": "^2.8.5",
        "events": "^3.3.0",
        "express": "^4.21.1",
        "fs": "^0.0.1-security",
        "golden-fleece": "^1.0.9",
        "json5": "^2.2.3",
        "mineflayer": "^4.20.1",
        "path": "^0.12.7",
        "prompt-sync": "^4.2.0",
        "readline": "^1.3.0",
        "socket.io": "^4.8.1",
        "winston": "^3.15.0",
        "ws": "^8.18.0"
    }
}
